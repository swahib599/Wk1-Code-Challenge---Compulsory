function speedDetector(speed) {
  if (speed <= 70) {
    console.log("OK");
  } else {
    let points = Math.floor((speed - 70) / 5);
    if (points >= 12) {
      console.log("License suspended");
    } else {
      console.log("Points:", points);
    }
  }
}

speedDetector(66);
speedDetector(85); 
speedDetector(130); 'License suspended'