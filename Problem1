function studentMarksGen(marks) {
    if (marks < 40) {
      return "not possible";
    } else if (marks < 50) {
      return "D";
    } else if (marks < 60) {
      return "C";
    } else if (marks < 80) {
      return "B";
    } else {
      return "A";
    }
  }
  console.log(studentMarksGen(40)); 
  console.log(studentMarksGen(55)); 
  console.log(studentMarksGen(70));
  console.log(studentMarksGen(85)); 
  console.log(studentMarksGen(30));